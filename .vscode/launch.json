    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Aruco Board Pose from Network",
            "type": "python",
            "request": "launch",
            "program": "src/visual_docking/aruco_board_pose_from_network.py",
            "console": "integratedTerminal",
            "args": [
                "--url", "http://192.168.1.100",
                "--calibration_file", "${workspaceFolder}/src/visual_docking/calibrations/cam1_70mm_1600_1200.yml",
                "--marker_size", "10.0",
                "--marker_spacing", "2.0",
                "--markers_count_x", "4",
                "--markers_count_y", "2",
                "--ros_ip", "192.168.1.2"
            ]
        },
        {
            "name": "Marker Pose from Network",
            "type": "python",
            "request": "launch",
            "program": "src/visual_docking/pose_from_network.py",
            "console": "integratedTerminal",
            "args": [
                "--url", "http://192.168.0.160",
                "--calibration_file", "${workspaceFolder}/src/visual_docking/calibrations/cam1_70mm_1600_1200.yml",
                "--marker_size", "20.0",
                "--ros_ip", "192.168.0.30"
            ]
        },
        {
            "name": "Cam Calibration",
            "type": "python",
            "request": "launch",
            "program": "src/visual_docking/calibration.py",
            "console": "integratedTerminal",
            "args": [
                "--url", "http://192.168.0.160",
                "--square_size", "4.0",
                "--width", "8",
                "--height", "6",
                "--save_file", "cam1_70mm_1600_1200.yml"]
        },
        {
            "name": "Clamp Controller Main with process",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/robot_clamp_controller/run.py",
            "console": "integratedTerminal",
            "args": ["-f", "C:\\Users\\leungp\\Documents\\GitHub\\integral_timber_joints\\external\\itj_design_study\\210128_RemodelFredPavilion\\results\\pavilion_process.json"]
        }
    ]
}